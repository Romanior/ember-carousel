{{yield
  (hash
    item=(component 'carousel-item'
      register=(action 'registerItem')
      unregister=(action 'unregisterItem')
      transitionInterval=transitionInterval
      allItems=carouselItems)
  )
  (hash
    previous=(action 'toLeft')
    next=(action 'toRight')
    to=(action 'jumpTo')
  )
  (hash
    allItems=carouselItems
  )
}}
